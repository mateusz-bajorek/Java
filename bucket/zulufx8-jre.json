    },
    "architecture": {
        "64bit": {
            "url": "https://cdn.azul.com/zulu/bin/zulu8.84.0.15-ca-fx-jre8.0.442-win_x64.zip",
            "hash": "33e2bdaa83d8b94de315592239a38d2e1c4f6097c0795b3269c09dd30d6e30ef",
            "extract_dir": "zulu8.84.0.15-ca-fx-jre8.0.442-win_x64"
        },
        "32bit": {
            "url": "https://cdn.azul.com/zulu/bin/zulu8.84.0.15-ca-fx-jre8.0.442-win_i686.zip",
            "hash": "41849dd044b277ac42e7654a62c15e60034536dbf799f53e227d514faa82c113",
            "extract_dir": "zulu8.84.0.15-ca-fx-jre8.0.442-win_i686"
        }
    },
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=8&bundle_type=jre&features=headfull,fx&javafx=true&ext=zip&os=windows&arch=x86&hw_bitness=64",
        "jsonpath": "$.name",
        "regex": "(?<name>zulu(?<version>[\\d.]+)-ca-fx-jre(?<java>[\\d.]+)-win)_x64.zip",
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://cdn.azul.com/zulu/bin/$matchName_x64.zip",
                "extract_dir": "$matchName_x64",
                "hash": {
                    "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=8&bundle_type=jre&features=headfull,fx&javafx=true&ext=zip&os=windows&arch=x86&hw_bitness=64",
                    "jp": "$.sha256_hash"
                }
            },
            "32bit": {
                "url": "https://cdn.azul.com/zulu/bin/$matchName_i686.zip",
                "extract_dir": "$matchName_i686",
                "hash": {
                    "url": "https://api.azul.com/zulu/download/community/v1.0/bundles/latest/?jdk_version=8&bundle_type=jre&features=headfull,fx&javafx=true&ext=zip&os=windows&arch=x86&hw_bitness=32",
                    "jp": "$.sha256_hash"
                }
            }
        }
    }
}
